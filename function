#include<stdio.h>
float area (int,int);
int main()
{
int b;
int h;
float result;
printf (" enter the base and height of the triangle");
scanf("%d %d", &b ,&h);
result = area(b,h);
printf ("the area of the triangle is %f" , result);
}

float area(int a, int b)
{
float out;
out = (a*b)/2;
return out;
}

